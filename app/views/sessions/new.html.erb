<h1>Sigin In</h1>
<% flash.each do |key, value| %>
  <%= content_tag(:div, value, class: key) %>
<% end %>
<%= form_for(@user, url: sign_in_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>